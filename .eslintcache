r\n        return;\r\n      }\r\n      setBreakTime((prev) => prev + amount);\r\n    } else {\r\n      if (workTime <= 60 && amount < 0) {\r\n        return;\r\n      }\r\n      setWorkTime((prev) => prev + amount);\r\n      if (!timerOn) {\r\n        setDisplayTime(workTime + amount);\r\n      }\r\n    }\r\n  }\r\n\r\n  const controlTime = () => {\r\n\r\n    let second = 1000;\r\n    let date = new Date().getTime();\r\n    let nextDate = new Date().getTime() + second;\r\n    let onBreakVariable = onBreak;\r\n\r\n    if (!timerOn) {\r\n      let interval = setInterval(() => {\r\n\r\n        date = new Date().getTime();\r\n        if (date > nextDate) {\r\n          setDisplayTime(prev => {\r\n            if (prev <= 0 && !onBreakVariable) {\r\n              playBreakSound();\r\n              onBreakVariable = true;\r\n              setOnBreak(true);\r\n              return breakTime;\r\n            } else if (prev <= 0 && onBreakVariable) {\r\n              playBreakSound();\r\n              onBreakVariable = false;\r\n              setOnBreak(false);\r\n              return workTime;\r\n            }\r\n            return prev - 1;\r\n          })\r\n\r\n          nextDate += second;\r\n        }\r\n\r\n      }, 30);\r\n      localStorage.clear();\r\n      localStorage.setItem('interval-id', interval);\r\n    }\r\n\r\n    if (timerOn) {\r\n      clearInterval(localStorage.getItem('interval-id'));\r\n    }\r\n    setTimerOn(!timerOn)\r\n  }\r\n  const resetTime = () => {\r\n    setDisplayTime(25 * 60);\r\n    setBreakTime(5 * 60);\r\n    setWorkTime(25 * 60)\r\n  }\r\n\r\n\r\n  return (\r\n    <section className=\"container-fluid secPomodoro\">\r\n      <article>\r\n        <h1 className=\"titlePomodoro\">Técnica Pomodoro</h1>\r\n        <div className=\"row\">\r\n          <div className=\"col-sm-12 col-md-6\">\r\n            <WorkTime\r\n              title={\"Tiempo de Trabajo\"}\r\n              changeTime={changeTime}\r\n              type={\"Trabajo\"}\r\n              time={workTime}\r\n              formatTime={formatTime} />\r\n          </div>\r\n          <div className=\"col-sm-12 col-md-6\">\r\n            <BreakTime\r\n              title={\"Tiempo de Descanso\"}\r\n              changeTime={changeTime}\r\n              type={\"Descanso\"}\r\n              time={breakTime}\r\n              formatTime={formatTime} />\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"container-fluid\">\r\n          <div className=\"container-fluid\">\r\n            <h3 className=\"textState mt-3\">{onBreak ? \"Descanso\" : \"Trabajo\"}</h3>\r\n            <h1 className=\"time2\">{formatTime(displayTime)}</h1>\r\n          </div>\r\n\r\n          <div className=\"container-fluid\">\r\n            <div className=\"buttonsTime\">\r\n              <button className=\"btnPlay\" onClick={controlTime}>\r\n                {\r\n                  timerOn \r\n                  ? \r\n                    (\r\n                      <h3><FontAwesomeIcon icon={faPauseCircle} /> Pausar</h3>\r\n                    )\r\n                  : \r\n                    (\r\n                      <h3><FontAwesomeIcon icon={faPlayCircle} /> Play</h3>\r\n                    )\r\n                }\r\n              </button>\r\n              <button className=\"btnReset\" onClick={resetTime}>\r\n                <h3>Reiniciar</h3>\r\n              </button>\r\n            </div>\r\n          </div>\r\n          <AlarmMessage />\r\n        </div>\r\n      </article>\r\n    </section>\r\n  )\r\n}","D:\\Archivos\\Estudios\\AcademiaGeekSerMasCapaces\\Entregables\\AcGeek_Repositories\\procrastinapp\\src\\components\\contents\\sectionMoreInfo\\SectionMoreInfo.jsx",[],"D:\\Archivos\\Estudios\\AcademiaGeekSerMasCapaces\\Entregables\\AcGeek_Repositories\\procrastinapp\\src\\components\\contents\\sectionChecklist\\SectionChecklist.jsx",["107","108"],"import React, { useState, useEffect } from 'react'\r\nimport Checklist from './Checklist'\r\nimport But